{
  "regions": ["sin1", "syd1", "iad1", "hkg1", "bom1"],
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "jpgstopdf.com" }],
      "destination": "https://www.jpgstopdf.com/:path*",
      "permanent": true
    },
    {
      "source": "/index.html",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/index.php",
      "destination": "/",
      "permanent": true
    }
  ]
}
